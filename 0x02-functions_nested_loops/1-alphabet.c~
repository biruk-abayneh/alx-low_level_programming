#include "main.h"

/**
 * main - check the code
 * print_alphabet - print alphabet
 * Return: Always 0.
 */
int main(void)
{
print_alphabet();
return (0);
}
/**
 * main - check the code
 * print_alphabet - print alphabet
 * Return: Always 0.
 */
void print_alphabet(void)
{
char lower[26] = {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h',
		  'i', 'j', 'k', 'l', 'm', 'n', 'o', 'p',
		  'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z'};
int i;

for (i = 0; i < 26; i++)
{
_putchar(lower[i]);
}
}
