#include <stdio.h>
#include "main.h"
/**
 * mul - multiply tow numbers
 * @a: integer
 * @b: integer
 * Return: answer
 */
/**
 * main - check the code
 *
 * Return: Always 0.
 */
int main(void)
{
    printf("%d\n", mul(98, 1024));
    printf("%d\n", mul(-402, 4096));
    return (0);
}
int mul(int a, int b)
{
int x;

x = a * b;
return (x);
}
