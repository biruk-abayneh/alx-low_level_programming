#include "main.h"

/**
 * main - check the code
 *
 * Return: Always 0.
 */
int main(void)
{
    print_square(10);
    print_square(0);
    return (0);
}

void print_square(int size)
{
  if (size == 0)
    putchar('\n');
  else
    {
      int i, j;

      for (i = 0; i < size; i++)
	{
	  for (j = 0; j < size; j++)
	    {
	      putchar('#');
	    }
	  putchar('\n');
	}
    }
}
